{
 "presets": [
   "@babel/preset-env",
   ["@babel/preset-react", {
    "runtime": "automatic"
   }],
   "@babel/preset-typescript"
 ],
 "plugins": [
    ["transform-class-properties"],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "pages": "./src/pages",
          "components": "./src/components",
          "constants": "./src/constants",
          "containers": "./src/containers",
          "styles": "./src/styles",
          "interfaces": "./src/interfaces",
          "dummy": "./src/dummy"
        }
      }
    ],
    [
      "emotion",
      {
        "sourceMap": true,
        "autoLabel": true,
        "labelFormat": "[dirname]_[local]",
        "cssPropOptimization": true
      }
    ]
 ]
}