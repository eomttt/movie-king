{
 "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "jsx": "react-jsx",
    "outDir": "build",
    "baseUrl": "src",
    "lib": [
      "dom",
      "esnext"
    ],
    "paths": {
      "pages/*": ["pages/*"],
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "styles/*": ["styles/*"],
      "interfaces/*": ["interfaces/*"],
      "dummy/*": ["dummy/*"],
      "apis/*": ["apis/*"],
      "hooks/*": ["hooks/*"],
      "lib/*": ["lib/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}

